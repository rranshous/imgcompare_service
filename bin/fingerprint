#!/usr/bin/env ruby

require 'ostruct'
require 'pathname'
lib_path = File.absolute_path(File.join(File.dirname(__FILE__),'../lib'))
$:.unshift lib_path
require 'image_fingerprinter'

raise 'must provide file path' if ARGV.first.nil?
file_path = Pathname.new ARGV.first

puts ImageFingerprinter.new.fingerprint OpenStruct.new(path: file_path)
